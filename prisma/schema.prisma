model account {
  id                     Int                      @id @default(autoincrement())
  email                  String                   @unique
  password               String?
  f_t_and_c              Boolean?
  account_manage         account_manage[]
  active                 Boolean?
  status                 Boolean?
  start_date             DateTime?
  end_date               DateTime?
  detail                 String?
  address                String?
  first_name             String?
